<!DOCTYPE html>
<html>
<head>
  <title>Mmo</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body class='wysihtml5-supported contrast-red fixed-header fixed-navigation'>
<header>
  <nav class='navbar navbar-default navbar-fixed-top oPageCentered2 top2'>
    <a class='toggle-nav btn pull-left hidden-md hidden-lg' href='#'>
      <i class='icon-reorder'></i>
    </a>

    <a class='navbar-brand' href='/'>
      <img class="logo img-responsive" width="164" alt="Flatty" src="/assets/logo.png"/>
      <img width="115" class="logo-xs" alt="Flatty" src="/assets/logo.png"/>
    </a>

    <h5 id="pTitle" class="page_title" align="center">
      <%= @page_title %>
    </h5>


    <ul class="nav" style="background-color: #fff !important;">
    </ul>
  </nav>
  <div id="top-bar0">
    <div class="oPageCentered2 top2">
      <div class="container">
      </div>
    </div>
  </div>
</header>

<div id='wrapper'>
  <!-- <div id='main-nav-bg'></div> -->
  <nav id='main-nav' class="main-nav-fixed affix-top">
    <div class='navigation'>
      <ul class='nav nav-stacked nav-left nav-adaptable'>
      </ul>
    </div>
  </nav>
</div>

<section id='content' class="oPageCentered">
  <div class='container top-bar-left-margin2'>
    <div class='row' id='content-wrapper'>
      <div class='page-header page-header-with-buttons fixed_dashboard_space'>
        <div class="col-md-12">
          <h1 class='pull-left'>
            <!-- <i class='icon-dashboard'></i> -->
            <span class='title-page'><%= @title_page.present? ? @title_page : 'Dashboard' %>  </span>
          </h1>
        </div>
      </div>
      <br/>

      <div class="row">
        <div class="fixed-stick-move-record col-md-12 pleft30 pmsg">
          <div class="row">
            <div class="top-bar-left-margin">&nbsp;</div>
            <div>
              <div class="cspacer"><h5><b class="textOrange caps"><%= @move_record.move_contract_name %></b></h5></div>
              <div class="cspacer"><h5><b class="textOrange caps"><%= @move_record.cargo_room %></b></h5></div>
              <div class="cspacer"><h5><b class="textOrange caps"><%= @move_record_origins[0].location.city %> - <%=
                  @move_record_destinations[0].location.city %></b></h5></div>
              <div class="cspacer"><h5>
                <b class="textOrange caps"><%= @move_record_dates[0].blank? ? 'TBD' : @move_record_dates[0].move_date.blank? ? 'TBD' : @move_record_dates[0].move_date.strftime("%d/%m/%Y") %></b>
              </h5></div>
              <div class="cspacer"><h5>
                <b class="textOrange caps"><%= @move_record_trucks.map { |v| v.truck.present? ? v.truck.description : 'TBD' }.compact.reject(&:blank?).join(', ') %></b>
              </h5></div>
              <div class="cspacer"><h5><b class="textOrange caps">$<%= @move_record.total_cost %></b></h5></div>
            </div>
          </div>
        </div>
        <div class="well pleft30">
          <% if !flash[:notice].blank? %>
              <div class="alert alert-success alert-dismissable move-record-message">
                <a class="close" data-dismiss="alert" href="#">Ã—
                </a>
                <h4>
                  <i class="icon-ok-sign"></i>
                  Successful
                </h4>

                <p><%= flash[:notice] %></p>
              </div>
          <% end %>
          <%= form_for @move_record do |f| %>
              <div class="row">
                <div class="col-md-12">
                  <legend id="SSwho">WHO?</legend>
                </div>
              </div>
              <% @move_record_clients.each do |move_client| %>
                  <div class="row client-row colpd">
                    <!--name-->
                    <div class="form-group col1-5 colmr">
                      <% if move_client.client.name.blank? %>
                          <%= label :client, :client, :name, id: "name", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :client, :name, id: "name" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :name, value: move_client.client.name, class: 'form-control', style: 'width:100%', required: true, placeholder: "Name", onfocus: "showLabel('name')" %>
                    </div>

                    <!--name-->
                    <div class="form-group col-md-1" style="display: none;">
                      <%= label :client, :client, 'Client Number' %><br>
                      <%= text_field "clients[#{move_client.client.id}]", :client_id, value: move_client.client.id, class: 'form-control', style: 'width:100%', readonly: true, disabled: true %>
                    </div>

                    <!--title-->
                    <div class="form-group col0-5 colmr">
                      <% if move_client.client.title.blank? %>
                          <%= label :client, :title, id: "title", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :title, id: "title" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :title, value: move_client.client.title, class: 'form-control', style: 'width:100%', placeholder: "Title", onfocus: "showLabel('title')" %>
                    </div>
                    <!--home_phone-->
                    <div class="form-group col1 colmr">
                      <% if move_client.client.home_phone.blank? %>
                          <%= label :client, :home_phone, id: "homePhone", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :home_phone, id: "homePhone" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :home_phone, value: move_client.client.home_phone, class: 'form-control', style: 'width:100%', placeholder: "Home Phone", onfocus: "showLabel('homePhone')" %>
                    </div>
                    <!--cell_phone-->
                    <div class="form-group col1 colmr">
                      <% if move_client.client.cell_phone.blank? %>
                          <%= label :client, :cell_phone, id: "cellPhone", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :cell_phone, id: "cellPhone" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :cell_phone, value: move_client.client.cell_phone, class: 'form-control', style: 'width:100%', placeholder: "Cell Phone", onfocus: "showLabel('cellPhone')" %>
                    </div>
                    <!--work_phone-->
                    <div class="form-group col1 colmr">
                      <% if move_client.client.work_phone.blank? %>
                          <%= label :client, :work_phone, id: "workPhone", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :work_phone, id: "workPhone" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :work_phone, value: move_client.client.work_phone, class: 'form-control', style: 'width:100%', placeholder: "Work Phone", onfocus: "showLabel('workPhone')" %>
                    </div>
                    <!--email-->
                    <div class="form-group col1-5">
                      <% if move_client.client.email.blank? %>
                          <%= label :client, :email, id: "email", class: "hhidden" %><br>
                      <% else %>
                          <%= label :client, :email, id: "email" %><br>
                      <% end %>
                      <%= text_field "clients[#{move_client.client.id}]", :email, value: move_client.client.email, class: 'form-control', style: 'width:100%', placeholder: "Email", onfocus: "showLabel('email')" %>
                    </div>
                  </div>
              <% end %>

              <!--move type-->
              <div class="row colpd">
                <div class="form-group col1 colmr">
                  <%= label :move_record, 'Move Type', id: "moveType", class: "hhidden" %><br>
                  <%= select :move_record, :move_type_id, options_from_collection_for_select(@move_type_data, "id", :description, @move_record.move_type_id), {include_blank: "Move Type"}, :class => 'form-control move_type_id text-grey', style: 'width:100%', data_id:
                      'move_type_id', onchange: "showLabel('moveType')" %>
                </div>
                <div class="form-group col1 colmr">
                  <%= label :move_type, 'Contract Name' %><br>
                  <%= text_field :move_record, :move_contract_name, value: @move_record.move_contract_name, class: 'form-control', style: 'width:100%', disabled: true, readonly: true %>
                </div>
                <div class="form-group col1 colmr">
                  <%= label :move_type, 'Contract Number' %><br>
                  <%= text_field :move_record, :move_contract_number, value: @move_record.move_contract_number, class: 'form-control', style: 'width:100%', disabled: true, readonly: true %>
                </div>
                <div class="form-group col2 colmr">
                  <%= label :move_type, 'Move type details', id: "moveTypeDetails", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_type_detail, value: @move_record.move_type_detail, class: 'form-control', style: 'width:100%', placeholder: "Move Type Details", onfocus: "showLabel('moveTypeDetails')" %>
                </div>
                <div class="form-group col1-5">
                  <%= label :move_type_details, 'Move type alert', id: "moveTypeAlert", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_type_alert_id, value: @move_record.move_type_alert_id, placeholder: "Move Type Alert", class: 'form-control', style: 'width:100%', list: 'move_type_alert_id', onfocus: "showLabel('moveTypeAlert')" %>
                  <datalist id="move_type_alert_id">
                    <%= options_from_collection_for_select(@move_type_alert, "description", :description,) %>
                  </datalist>
                </div>
              </div>

              <!--move source-->
              <div class="row colpd">
                <div class="form-group col1 colmr">
                  <%= label :move_record, :source, id: "source", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_source_id, value: @move_record.move_source_id, placeholder: "Source", class: 'form-control text-grey', style: 'width:100%', list: 'move_source_id', onfocus: "showLabel('source')" %>
                  <datalist id="move_source_id">
                    <%= options_from_collection_for_select(@move_source, "description", :description,) %>
                  </datalist>
                </div>

                <div class="form-group col1 colmr">
                  <%= label :move_record, :keyword, id: "keyword", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_keyword_id, value: @move_record.move_keyword_id, placeholder: "Keyword", class: 'form-control text-grey', style: 'width:100%', list: 'move_keyword_id', onfocus: "showLabel('keyword')" %>
                  <datalist id="move_keyword_id">
                    <%= options_from_collection_for_select(@move_keyword, "description", :description,) %>
                  </datalist>
                </div>

                <div class="form-group col1 colmr">
                  <%= label :move_record, :webpage, id: "webpage", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_webpage_id, value: @move_record.move_webpage_id, placeholder: "Webpage", class: 'form-control text-grey', style: 'width:100%', list: 'move_webpage_id', onfocus: "showLabel('webpage')" %>
                  <datalist id="move_webpage_id">
                    <%= options_from_collection_for_select(@move_webpage, "description", :description,) %>
                  </datalist>
                </div>

                <div class="form-group col1 colmr">
                  <%= label :move_record, :referral, id: "referral", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_referral_id, value: @move_record.move_referral_id, placeholder: "Referral", class: 'form-control text-grey', style: 'width:100%', list: 'move_referral_id', onfocus: "showLabel('referral')" %>
                  <datalist id="move_referral_id">
                    <%= options_from_collection_for_select(@move_referral, "description", :description,) %>
                  </datalist>
                </div>

                <div class="form-group col1-5 colmr">
                  <%= label :move_record, :source_details, id: "sourceDetails", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_source_detail, class: 'form-control', style: 'width:100%', placeholder: "Source Details", onfocus: "showLabel('sourceDetails')" %>
                </div>

                <div class="form-group col1">
                  <%= label :move_record, :source_alert, id: "sourceAlert", class: "hhidden" %><br>
                  <%= text_field :move_record, :move_source_alert_id, value: @move_record.move_source_alert_id, placeholder: "Source Alert", class: 'form-control text-grey', style: 'width:100%', list: 'move_source_alert_id', onfocus: "showLabel('sourceAlert')" %>
                  <datalist id="move_source_alert_id">
                    <%= options_from_collection_for_select(@move_alert, "description", :description,) %>
                  </datalist>
                </div>
              </div>

              <!--who note-->
              <div class="row colpd">
                <div class="form-group col7">
                  <%= label :note, 'Note', id: "note1", class: "hhidden" %><br>

                  <div class="left-inner-addon">
                    <%= text_area :move_record, :who_note, class: 'form-control textarea-control text-grey', style: 'width:100%', rows: 1, onfocus: "showLabel('note1')", placeholder: "Note" %>
                  </div>
                </div>
              </div>
              <br/>

              <!--what-->
              <div class="row">
                <div class="col-md-12">
                  <div>
                    <legend id="SSwhat">
                      WHAT?
                      <div class="pull-right">
                        <a data-toggle="modal" href="#modal-cargo">Cargo Calculator
                          <i class="icon-collapse-top"></i></a>
                      </div>
                    </legend>
                  </div>
                </div>
              </div>
              <div class="row colpd">
                <div class="form-group col1 colmr">
                  <%= label :cargo, :cargo_type, id: "cargoType", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_type_id, value: @move_record.cargo_type_id, placeholder: "Cargo Type", class: 'form-control', style: 'width:100%', list: 'cargo_type_id', onfocus: "showLabel('cargoType')" %>
                  <datalist id="cargo_type_id">
                    <%= options_from_collection_for_select(@cargo_type, "description", :description,) %>
                  </datalist>
                </div>

                <div class="form-group col0-8 colmr">
                  <%= label :cargo, 'Descriptor', id: "descriptor", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_descriptor, class: 'form-control', style: 'width:100%', placeholder: "Descriptor", onfocus: "showLabel('descriptor')" %>
                </div>

                <div class="form-group col0-5 colmr">
                  <%= label :cargo, '#Rooms', id: "noRooms", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_room, class: 'form-control', style: 'width:100%', placeholder: "#Rooms", onfocus: "showLabel('noRooms')" %>
                </div>

                <div class="form-group col0-5 colmr">
                  <%= label :cargo, 'Weigth', id: "weight", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_weight, class: 'form-control', style: 'width:100%', placeholder: "Weight", onfocus: "showLabel('weight')" %>
                </div>

                <div class="form-group col0-5 colmr">
                  <%= label :cargo, 'Cubic', id: "cubic", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_cubic, class: 'form-control', style: 'width:100%', placeholder: "Cubic", onfocus: "showLabel('cubic')" %>
                </div>

                <div class="form-group col2 colmr">
                  <%= label :cargo, :cargo_details, id: "cargoDetails", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_detail, class: 'form-control', style: 'width:100%', placeholder: "Cargo Details", onfocus: "showLabel('cargoDetails')" %>
                </div>

                <div class="form-group col1">
                  <%= label :cargo, :cargo_alert, id: "cargoAlert", class: "hhidden" %><br>
                  <%= text_field :move_record, :cargo_alert_id, value: @move_record.cargo_alert_id, placeholder: "Cargo Alert", class: 'form-control', style: 'width:100%', list: 'cargo_alert_id', onfocus: "showLabel('cargoAlert')" %>
                  <datalist id="cargo_alert_id">
                    <%= options_from_collection_for_select(@cargo_alert, "description", :description,) %>
                  </datalist>
                </div>
              </div>
              <div class="row colpd">
                <div class="form-group col7">
                  <%= label :cargo, :cargo_description, id: "cargoDescription", class: "hhidden" %><br>
                  <%= text_area :move_record, :cargo_description, class: 'form-control', style: 'width:100%', rows: 1, placeholder: "Cargo Description", onfocus: "showLabel('cargoDescription')" %>
                </div>
              </div>
              <div class="row colpd">
                <div class="form-group col7">
                  <%= label :note, 'Note', id: "note2", class: "hhidden" %><br>
                  <%= text_area :move_record, :what_note, class: 'form-control textarea-control', style: 'width:100%',
                                rows: 1, onfocus: "showLabel('note2')", placeholder: "Note" %>
                </div>
              </div>
              <br/>

              <!--where-->
              <div class="row">
                <div class="col-md-12">
                  <div>
                    <legend id="SSwhere">
                      WHERE?
                      <div class="pull-right">
                        <a target="_blank" href="#" class="move-map" type="street">Street View
                          <i field="number_of_origin" class="icon-external-link"></i></a>
                        <a target="_blank" href="#" class="move-map" type="gps">GPS View
                          <i field="number_of_origin" class="icon-external-link"></i></a>
                        <a href="javascript:void(0)" style="cursor: text">Origin</a>
                        <i field="number_of_origin" class="fa fa-plus move-add-line" style="cursor: pointer"></i>
                        <a href="javascript:void(0)" style="cursor: text">Destination</a>
                        <i field="number_of_destination" class="fa fa-plus move-add-line" style="cursor: pointer"></i>
                      </div>
                    </legend>
                  </div>
                </div>
              </div>
              <% @move_record_origins.each do |origin| %>
                  <div class="row origin-row colpd">
                    <div class="row colpd">
                      <div class="form-group colfl colmr">
                        <label></label><br/><label><b>ORIGIN&nbsp;</b></label>
                      </div>
                      <div class="form-group col0-5">
                        <%= label :move_record, :no, id: "originNo1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :no, value: origin.location.no, class: 'form-control data-no', style: 'width:100%', placeholder: "No", onfocus: "showLabel('originNo1')" %>
                      </div>

                      <div class="form-group col1 colmr">
                        <%= label :move_record, :street, id: "originStreet1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :street, value: origin.location.street, class: 'form-control data-street', style: 'width:100%', placeholder: "Street", onfocus: "showLabel('originStreet1')" %>
                      </div>

                      <div class="form-group col0-5 colmr">
                        <%= label :move_record, :apartment, "Apt", id: "originApartment1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :apartment, value: origin.location.apartment, class: 'form-control', style: 'width:100%', placeholder: "Apt", onfocus: "showLabel('originApartment1')" %>
                      </div>

                      <div class="form-group col0-5 colmr">
                        <%= label :move_record, :entry_number, "Entry #", id: "originEntryNumber1", class: "hhidden" %>
                        <br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :entry_number, value: origin.location.entry_number, class: 'form-control', style: 'width:100%', placeholder: "Entry #", onfocus: "showLabel('originEntryNumber1')" %>
                      </div>

                      <div class="form-group col1 colmr">
                        <%= label :move_record, :building, id: "originBuilding1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :building, value: origin.location.building, class: 'form-control', style: 'width:100%', placeholder: "Building", onfocus: "showLabel('originBuilding1')" %>
                      </div>

                      <div class="form-group col1-5 colmr">
                        <%= label :move_record, :truck_group, id: "originTruckGroup1", class: "hhidden" %><br>
                        <%= select "move_record_origin[#{origin.location.id}]", :calendar_truck_group_id, options_from_collection_for_select(@all_calendar_groups, "id", :description, origin.location.calendar_truck_group_id), {include_blank: "Truck Group"}, :class => 'form-control move-truck-calendar text-grey', style: 'width:100%', onchange: "showLabel('originTruckGroup1')" %>
                      </div>

                      <div class="form-group col1">
                        <%= label :move_record, :city, id: "originCity1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :city, value: origin.location.city, class: 'form-control data-city', style: 'width:100%', placeholder: "City", onfocus: "showLabel('originCity1')" %>
                      </div>

                    </div>
                    <div class="row colpd">

                      <div class="form-group col0-5">
                        <%= label :move_record, :locale, id: "originLocale1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :locale, value: origin.location.locale, class: 'form-control', style: 'width:100%', placeholder: "Locale", onfocus: "showLabel('originLocale1')" %>
                      </div>

                      <div class="form-group col0-5">
                        <%= label :move_record, :province, id: "originProvince1", class: "hhidden" %><br>
                        <select name="move_record_origin[<%= origin.location.id %>][province]" class="form-control data-province text-grey" style="width:100%" onchange="showLabel('originProvince1')" %>
                          <option value="">Prov.</option>
                          <% @provinces.each do |prov| %>
                              <option value="<%= prov.description %>"> <%= prov.description %></option>
                          <% end %>
                        </select>
                      </div>

                      <div class="form-group col0-5 colmr">
                        <%= label :move_record, :postal_code, "PC", id: "originPostalCode1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :postal_code, value: origin.location.postal_code, class: 'form-control data-postal-code', style: 'width:100%', placeholder: "PC", onfocus: "showLabel('originPostalCode1')" %>
                      </div>

                      <div class="form-group colfl">&nbsp;&nbsp;</div>

                      <div class="form-group col4 colmr">
                        <%= label :move_record, :access_details, "Origin Access Details", id: "originAccessDetails1", class: "hhidden" %>
                        <br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :access_details, value: origin.location.access_details, class: 'form-control', style: 'width:100%', placeholder: "Origin Access Details", onfocus: "showLabel('originAccessDetails1')" %>
                      </div>

                      <div class="form-group col1">
                        <%= label :move_record, :access_alert, id: "originAccessAlert1", class: "hhidden" %><br>
                        <%= text_field "move_record_origin[#{origin.location.id}]", :access_alert_id, value: origin.location.access_alert_id, class: 'form-control', style: 'width:100%', list: 'origin_access_alert_id', placeholder: "Access Alert", onfocus:
                            "showLabel
                            ('originAccessAlert1')" %>
                        <datalist id="origin_access_alert_id">
                          <%= options_from_collection_for_select(@access_alert, "description", :description,) %>
                        </datalist>
                      </div>
                    </div>
                  </div>
              <% end %>

              <% @move_record_destinations.each do |destination| %>
                  <div class="row destination-row colpd">
                    <div class="row colpd">
                      <div class="form-group colfl colmr">
                        <label></label><br/><label><b>DEST. &nbsp;&nbsp;&nbsp;</b></label>
                      </div>

                      <div class="form-group col0-5">
                        <%= label :move_record, :no, id: "destinationNo1", class: "hhidden" %><br>
                        <%= text_field "move_record_destination[#{destination.location.id}]", :no, value: destination.location.no, class: 'form-control data-no', style: 'width:100%', placeholder: "No", onfocus: "showLabel('destinationNo1')" %>
                      </div>

