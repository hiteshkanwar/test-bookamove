<legend>Move Webpage</legend>
<div class="row">
  <div class="col-md-12">
    <%= link_to 'New Move Type', new_move_webpage_path, class: 'btn btn-primary' %>
  </div>
</div>
<hr>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-bordered table-hover">
      <tr>
        <th>Description</th>
        <th>Is Active?</th>
        <th>Actions</th>
      </tr>
      <% @move_webpages.each do |move_webpage| %>
          <tr>
            <td><%= move_webpage.description %></td>
            <td><%= check_box_tag 'active', move_webpage.active, move_webpage.active, disabled: true %></td>
            <td>
              <%= link_to 'Edit', edit_move_webpage_path(move_webpage) %>
              <%= link_to 'Destroy', move_webpage, method: :delete, data: {confirm: 'Are you sure?'} %>
            </td>
          </tr>
      <% end %>
    </table>
  </div>
</div>
