<div class="row">
  <div class="form-group col-md-2">
    <%= label :source, :source, class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_source ? @enable_icon : @disable_icon %>
      <i field="show_move_source" class="<%= css %>"></i>
      <%= select :source, :source, {}, {include_blank: true}, :class => 'form-control space-select-addon change-select', style: 'width:100%', disabled: true %>
      <% if validate_special_permission('edit.contract_field_default_values', @current_actions) %>
        <i class="icon-edit-sign edit-dropdown-contract-settings" field="move_sources" data-default="<%= @settings.move_source_id %>" data-default-key="move_source_id"></i>
      <% end %>
    </div>
  </div>

  <div class="form-group col-md-2">
    <%= label :source, 'Sub-Source', class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_source ? @enable_icon : @disable_icon %>
      <i field="show_move_source" class="<%= css %>"></i>
      <%= select :source, :source, {}, {include_blank: true}, :class => 'form-control space-select-addon change-select', style: 'width:100%', disabled: true %>
      <% if validate_special_permission('edit.contract_field_default_values', @current_actions) %>
        <i class="icon-edit-sign edit-dropdown-contract-settings" field="move_subsources" data-default="<%= @settings.move_source_id %>" data-default-key="move_subsource_id"></i>
      <% end %>
    </div>
  </div>

  <div class="form-group col-md-2">
    <%= label :referral, :referral, class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_referral ? @enable_icon : @disable_icon %>
      <i field="show_move_referral" class="<%= css %>"></i>
      <%= select :referral, :referral, {}, {include_blank: true}, :class => 'form-control space-select-addon change-select', style: 'width:100%', disabled: true %>
