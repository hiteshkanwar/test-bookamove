        <i class="icon-edit-sign edit-dropdown-contract-settings" field="move_keywords" data-default="<%= @settings.move_keyword_id %>" data-default-key="move_keyword_id"></i>
      <% end %>
    </div>
  </div>

  <div class="form-group col-md-1" style="padding-left: 3px; padding-right: 3px">
    <%= label :webpage, :webpage, class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_webpage ? @enable_icon : @disable_icon %>
      <i field="show_move_webpage" class="<%= css %>"></i>
      <%= select :webpage, :webpage, {}, {include_blank: true}, :class => 'form-control space-select-addon change-select', style: 'width:100%', disabled: true %>
      <% if validate_special_permission('edit.contract_field_default_values', @current_actions) %>
        <i class="icon-edit-sign edit-dropdown-contract-settings" field="move_webpages" data-default="<%= @settings.move_webpage_id %>" data-default-key="move_webpage_id"></i>
      <% end %>
    </div>
  </div>

  <div class="form-group col-md-2">
    <%= label :source_details, :source_alert, class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_source_alert ? @enable_icon : @disable_icon %>
      <i field="show_move_source_alert" class="<%= css %>"></i>
      <%= select :source_details, :source_alert, {}, {include_blank: true}, :class => 'form-control space-select-addon change-select', style: 'width:100%', disabled: true %>
      <% if validate_special_permission('edit.contract_field_default_values', @current_actions) %>
