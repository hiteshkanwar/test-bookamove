    <div class="left-inner-addon">
      <% css = @settings.show_move_source_detail ? @enable_icon : @disable_icon %>
      <i field="show_move_source_detail" class="<%= css %>"></i>
      <%= text_field :source_details, :source_details, disabled: true, class: 'form-control', style: 'width:100%' %>
    </div>
  </div>

  <div class="form-group col-md-1" style="padding-left: 3px; padding-right: 3px">
    <%= label :keyword, :keyword, class: "labels" %><br>

    <div class="left-inner-addon">
      <% css = @settings.show_move_keyword ? @enable_icon : @disable_icon %>
