        <i class="icon-edit-sign edit-dropdown-contract-settings" field="move_referrals" data-default="<%= @settings.move_referral_id %>" data-default-key="move_referral_id"></i>
      <% end %>
    </div>
  </div>

  <div class="form-group col-md-2">
