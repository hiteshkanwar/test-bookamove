          <div class="col7">
            <div>
              <legend id="SSwhat" class="clearfix">
              <span onclick="expand_what()" class="pointer col1"><i class="fa fa-suitcase" aria-hidden="true"></i> WHAT</span>
              <span class="what_info <% if params[:new] == "true" %>hide<% end %>">
                <b class="textOrange caps font_normal"><%= @move_record.cargo_type_id %></b>
              </span>

              <div class="pull-right">
                <span class="what_links <% if not params[:new] == "true" %>hide<% end %>">
                  <a data-toggle="modal" class="no_underline" href="#modal-cargo">Cargo Calculator <i class="icon-collapse-top"></i></a>
                </span>
                <% if not params[:new] == "true" %>
                    <a class="btn btn-xs btn-link no_underline expand-what" href="javascript:void(0)" onclick="expand_what()"><i class="fa fa-chevron-down"></i></a>
