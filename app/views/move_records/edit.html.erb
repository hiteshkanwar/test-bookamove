          <div class="form-group col1 colmr">
            <%= label :move_record, :keyword, id: "keyword", class: "hhidden labels" %><br>
            <%= text_field :move_record, :move_keyword_id, value: @move_record.move_keyword_id, placeholder: "Keyword", class: 'form-control text-grey', style: 'width:100%', list: 'move_keyword_id', onfocus: "showLabel('keyword')" %>
            <datalist id="move_keyword_id">
              <%= options_from_collection_for_select(@move_keyword, "description", :description,) %>
            </datalist>
          </div>

          <div class="form-group col1 colmr">
            <%= label :move_record, :webpage, id: "webpage", class: "hhidden labels" %><br>
            <%= text_field :move_record, :move_webpage_id, value: @move_record.move_webpage_id, placeholder: "Webpage", class: 'form-control text-grey', style: 'width:100%', list: 'move_webpage_id', onfocus: "showLabel('webpage')" %>
            <datalist id="move_webpage_id">
              <%= options_from_collection_for_select(@move_webpage, "description", :description,) %>
            </datalist>
          </div>

          <div class="form-group col1">
            <%= label :move_record, :source_alert, id: "sourceAlert", class: "hhidden labels" %><br>
            <%= text_field :move_record, :move_source_alert_id, value: @move_record.move_source_alert_id, placeholder: "Source Alert", class: 'form-control text-grey', style: 'width:100%', list: 'move_source_alert_id', onfocus: "showLabel('sourceAlert')", onchange: "updateAlerts('move_record_move_source_alert_id', 'source_alert')" %>
            <datalist id="move_source_alert_id">
              <%= options_from_collection_for_select(@move_alert, "description", :description,) %>
            </datalist>
          </div>
        </div>

        <!--who note-->
        <div class="row colpd">
          <div class="form-group col7">
            <%= label :note, 'Source Note', id: "note1", class: "hhidden labels" %><br>
            <div class="left-inner-addon">
              <%= text_area :move_record, :who_note, class: 'form-control text-grey', style: 'width:100%', rows: 1, onfocus: "showLabel('note1')", placeholder: "Source Note" %>
            </div>

          </div>
        </div>

        <!--review-->
        <div class="row colpd">
          <div class="form-group col3 colmr">
            <%= label :note, 'Customer Review', id: "review", class: "hhidden labels" %><br>
            <div class="left-inner-addon">
              <%= text_area :move_record, :review_text, class: 'form-control text-grey', style: 'width:100%', rows: 1, onfocus: "showLabel('review')", placeholder: "Customer Review" %>
            </div>
          </div>

          <div class="form-group col1-5 colmr">
            <%= label :note, 'Review Link', id: "review_link", class: "hhidden labels" %><br>
            <div class="left-inner-addon">
              <%= text_field :move_record, :review_link, class: 'form-control text-grey', style: 'width:100%', placeholder: "Review Link", onfocus: "showLabel('review_link')" %>
            </div>
          </div>

          <div class="form-group col1-5 colmr">
            <%= label :note, 'Review Date', id: "review_date", class: "hhidden labels" %><br>
            <div class="left-inner-addon">
              <%= text_field :move_record, :review_date, class: 'form-control text-grey date-pick', style: 'width:100%', placeholder: "Review Date", onfocus: "showLabel('review_date')" %>
            </div>
          </div>

          <div class="form-group col1">
            <label class="labels"></label>
            <br>
            <% if @move_record.review_status.blank? %>
                <button type="button" class="btn h_30 grey no_pointer whitesmoke-bg" style="width: 100%">not reviewed</button>
            <% elsif @move_record.review_status == "positive" %>
                <button type="button" class="btn h_30 no_pointer btn-success green-background small" style="min-width: 100%"><% if not @move_record.review_chosen_site.blank? %>positive <%= @move_record.review_chosen_site %><% end %></button>
            <% elsif @move_record.review_status == "negative" %>
                <button type="button" class="btn h_30 no_pointer btn-danger red-bg" style="width: 100%"></button>
            <% end %>
          </div>
        </div>


        </div>

        <br/>

        <% end %>

        <!-- WHAT -->
