            <%= label :cargo, :cargo_type, id: "cargoType", class: "hhidden labels" %><br>
            <%= text_field :move_record, :cargo_type_id, value: @move_record.cargo_type_id, placeholder: "Cargo Type", class: 'form-control', style: 'width:100%', list: 'cargo_type_id', onfocus: "showLabel('cargoType')" %>
