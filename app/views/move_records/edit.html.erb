                    <a id="client_cell_phone" class="tel hidden" href="#SScontact" onclick="do_call(<%= move_client.client.cell_phone %>)"><i class="fa fa-phone-square" aria-hidden="true"></i>
                      Call</a><br>
                    <%= text_field "clients[#{move_client.client.id}]", :cell_phone, value: move_client.client.cell_phone, class: 'form-control cell_phone', style: 'width:100%', placeholder: "Cell Phone", onfocus: "showLabel('cellPhone')" %>
                <% else %>
                    <%= label :client, :cell_phone, id: "cellPhone", class: "labels" %> &nbsp;
                    <a id="client_cell_phone" class="tel" href="#SScontact" onclick="do_call(<%= move_client.client.cell_phone %>)"><i class="fa fa-phone-square" aria-hidden="true"></i>
                      Call</a><br>
                    <%= text_field "clients[#{move_client.client.id}]", :cell_phone, value: move_client.client.cell_phone, class: 'form-control2 cell_phone', style: 'width:100%', placeholder: "Cell Phone", onfocus: "showLabel('cellPhone')" %>
                <% end %>
              </div>
              <!--work_phone-->
              <div class="form-group col1 colmr">
                <% if move_client.client.work_phone.blank? %>
                    <%= label :client, :work_phone, id: "workPhone", class: "hhidden labels" %> &nbsp;
                    <a id="client_work_phone" class="tel hidden" href="#SScontact" onclick="do_call(<%= move_client.client.work_phone %>)"><i class="fa fa-phone-square" aria-hidden="true"></i>
                      Call</a><br>
                    <%= text_field "clients[#{move_client.client.id}]", :work_phone, value: move_client.client.work_phone, class: 'form-control work_phone', style: 'width:100%', placeholder: "Work Phone", onfocus: "showLabel('workPhone')" %>
                <% else %>
                    <%= label :client, :work_phone, id: "workPhone", class: "labels" %> &nbsp;
                    <a id="client_work_phone" class="tel" href="#SScontact" onclick="do_call(<%= move_client.client.work_phone %>)"><i class="fa fa-phone-square" aria-hidden="true"></i>
                      Call</a><br>
                    <%= text_field "clients[#{move_client.client.id}]", :work_phone, value: move_client.client.work_phone, class: 'form-control2 work_phone', style: 'width:100%', placeholder: "Work Phone", onfocus: "showLabel('workPhone')" %>
                <% end %>
              </div>
              <!--email-->
              <div class="form-group col2">
                <%= label :client, :email, id: "email", class: "hhidden labels" %><br>
