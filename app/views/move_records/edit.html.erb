<div class="row">

  <div class="fixed-stick-move-record msg">
  </div>
  <div class="well pleft30 mtop10">
    <!-- confirm flash -->
    <% if !flash[:notice].blank? %>
        <div class="alert alert-success alert-dismissable move-record-message mtop0">
          <a class="close" data-dismiss="alert" href="#">×
          </a>
          <h4>
            <i class="icon-ok-sign"></i>
            Successful
          </h4>

          <p><%= flash[:notice] %></p>
        </div>
    <% end %>
    <!-- error flash -->
    <% if !flash[:error].blank? %>
        <div class="alert alert-danger alert-dismissable move-record-message">
          <a class="close" data-dismiss="alert" href="#">×
          </a>
          <h4>
            <i class="icon-ok-sign"></i>
            Unsuccessful
          </h4>

          <p><%= flash[:error] %></p>
        </div>
    <% end %>
    <%= form_for @move_record do |f| %>

        <% if ( (not validate_special_permission('view.moves_without_who_how_much_or_street_address_number', @current_actions)) and (not validate_special_permission('view.moves_without_who_street_address_number', @current_actions)) or (validate_special_permission('view.moves_in_full', @current_actions)) or (@is_within_24h and (validate_special_permission('view.moves_in_full_only_within_24_hrs_of_move_time', @current_actions))) and not @my_move ) or ( @my_move and (not validate_special_permission('view.my_moves_without_who_street_address_number', @current_actions)) or (validate_special_permission('view.my_moves_in_full', @current_actions)) or (@is_within_24h and (validate_special_permission('view.my_moves_in_full_only_within_24_hrs_of_move_time', @current_actions))) ) %>

        <!-- WHO -->
        <div class="row colpd">
          <div class="col7">
            <legend id="SSwho" class="clearfix">
              <span onclick="expand_who()" class="pointer col1"><i class="fa fa-user" aria-hidden="true"></i> WHO</span>
              <span class="who_info <% if params[:new] == "true" %>hide<% end %>">
                <b class="textOrange caps who_text font_normal"><%= @move_record_clients[0].client.name %></b>
              </span>
              <div class="pull-right">
                <% if not params[:new] == "true" %>
                  <a class="btn btn-xs btn-link no_underline expand-who" href="javascript:void(0)" onclick="expand_who()"><i class="fa fa-chevron-down"></i></a>
                <% else %>
                    <a class="btn btn-xs btn-link no_underline expand-who" href="javascript:void(0)" onclick="expand_who()"><i class="fa fa-chevron-up"></i></a>
                <% end %>
              </div>
            </legend>
          </div>
        </div>

        <% if not params[:new] == "true" %>
            <div class="who-section" style="display: none;">
        <% else %>
            <div class="who-section">
        <% end %>
        <% @move_record_clients.each do |move_client| %>
            <div class="row client-row colpd">
              <!--name-->
              <div class="form-group col1-5 colmr">
                <%= label :client, :client, :name, id: "name", class: "hhidden labels" %><br>
                <%= text_field "clients[#{move_client.client.id}]", :name, value: move_client.client.name, class: 'form-control rname', style: 'width:100%', required: true, placeholder: "Name", onfocus: "showLabel('name')" %>
              </div>

              <!--id-->
              <div class="form-group col-md-1" style="display: none;">
                <%= label :client, :client, 'Client Number', class: "labels" %><br>
                <%= text_field "clients[#{move_client.client.id}]", :client_id, value: move_client.client.id, class: 'form-control', style: 'width:100%', readonly: true, disabled: true %>
              </div>

              <!--title-->
              <div class="form-group col0-5 colmr">
                <%= label :client, :title, id: "title", class: "hhidden labels" %><br>
                <%= text_field "clients[#{move_client.client.id}]", :title, value: move_client.client.title, class: 'form-control', style: 'width:100%', placeholder: "Title", onfocus: "showLabel('title')" %>
